{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=TelegramDb;Username=postgres;Password=your_password"
  },
  "Bots": [
    {
      "Name": "Bot1",
      "Token": "your_token",
      "DefaultChatId": "your_chat_id"
    }
  ],
  "Settings": {
    "MessagesPerMinute": 20,
    "RetryCount": 3,
    "RetryDelaySeconds": 5
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}